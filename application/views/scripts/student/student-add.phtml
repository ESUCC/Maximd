	       $("#ward_surrogate_nn1").prop("checked", false)
	       $("#ward_surrogate_nn2").prop("checked", false)
	       $("#ward_surrogate_other").val("")
	}

       return false;
    });

    $(document).on("click", ".StudentEditSave", function () {
        if ($("#studentForm").valid()) $("#studentForm")[0].submit();
        return false;
    });
  });
</script>


